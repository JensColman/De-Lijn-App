<% include header %>

<div class="routeplanner">
  <h2>Waar wil je naartoe?</h2>
  <form action="">
      <input type="text" name="vertrek" placeholder="Van">
      <input type="text" name="aankomst" placeholder="Naar">
      <select name="vertrek-aankomst">
            <option value="aankomst">Aankomst</option>
            <option value="vertrek">Vertrek</option>
      </select>
      <input type="time" name="tijd" value="13:00">
      <input type="submit" value="Plan route">
  </form>
</div>
